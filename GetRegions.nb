 RowBox[{
  RowBox[{"InnerNormals", "[", 
   RowBox[{"pts_", "?", "MatrixQ"}], "]"}], " ", ":=", "\n", "  ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "np", ",", "dim", ",", "file", ",", "str", ",", "nf", ",", "fs", ",", 
      "ns", ",", 
      RowBox[{"mp", "=", 
       RowBox[{
        RowBox[{"Total", "[", "pts", "]"}], "/", 
        RowBox[{"Length", "[", "pts", "]"}]}]}], ",", "in", ",", "out"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"np", ",", "dim"}], "}"}], "=", 
      RowBox[{"Dimensions", "@", "pts"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"file", " ", "=", " ", 
      RowBox[{"OpenWrite", "[", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"WriteString", "[", 
      RowBox[{"file", ",", 
       RowBox[{
        RowBox[{"ToString", "[", "dim", "]"}], " ", "<>", 
        " ", "\"\< # dimension\\n\>\""}]}], "]"}], ";", 
     RowBox[{"WriteString", "[", 
      RowBox[{"file", ",", 
       RowBox[{
        RowBox[{"ToString", "[", "np", "]"}], " ", "<>", 
        " ", "\"\< # number of points\\n\>\""}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"str", "=", 
      RowBox[{"StringRiffle", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"StringRiffle", "[", 
           RowBox[{
            RowBox[{"ToString", " ", "/@", "#"}], ",", "\"\< \>\""}], "]"}], 
          "&"}], "/@", "pts"}], ",", "\"\<\\n\>\""}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"WriteString", "[", 
      RowBox[{"file", ",", "str"}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"in", "=", 
      RowBox[{"Close", "[", "file", "]"}]}], ";", "\n", 
     RowBox[{"out", "=", 
      RowBox[{"Close", "[", 
       RowBox[{"OpenWrite", "[", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Run", "[", 
      RowBox[{"\"\<qconvex Fv\>\"", " ", "<>", " ", "\"\< < \>\"", " ", "<>", 
       " ", "in", " ", "<>", " ", "\"\< > \>\"", " ", "<>", " ", "out"}], 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"file", "=", 
      RowBox[{"OpenRead", "[", "out", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"nf", " ", "=", " ", 
      RowBox[{"Read", "[", 
       RowBox[{"file", ",", "Number"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"ns", "=", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"-", 
          RowBox[{"Cross", "@@", 
           RowBox[{"Rest", "[", "#", "]"}]}]}], 
         RowBox[{
          RowBox[{"First", "[", "#", "]"}], ".", 
          RowBox[{"(", 
           RowBox[{"Cross", "@@", 
            RowBox[{"Rest", "[", "#", "]"}]}], ")"}]}]], "&"}], "/@", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"PickBasis", "[", 
           RowBox[{"Differences", "[", 
            RowBox[{"Prepend", "[", 
             RowBox[{
              RowBox[{"pts", "[", 
               RowBox[{"[", "#", "]"}], "]"}], ",", "mp"}], "]"}], "]"}], 
           "]"}], "&"}], "/@", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"Rest", "[", 
             RowBox[{"#", "+", "1"}], "]"}], "&"}], "/@", 
           RowBox[{"ReadList", "[", 
            RowBox[{"file", ",", "Number", ",", 
             RowBox[{"RecordLists", "->", "True"}]}], "]"}]}], ")"}]}], 
        ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Close", "[", "file", "]"}], ";", 
     RowBox[{
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"LCM", "@@", 
          RowBox[{"Abs", "[", 
           RowBox[{"Denominator", "[", "#", "]"}], "]"}]}], 
         RowBox[{"GCD", "@@", 
          RowBox[{"Abs", "[", 
           RowBox[{"Numerator", "[", "#", "]"}], "]"}]}]], "*", "#"}], "&"}], 
      "/@", "ns"}]}]}], "]"}]}]}], "Code",
