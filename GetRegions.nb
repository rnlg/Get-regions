Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"GetRegions", "::", "usage"}], 
   "=", "\"\<GetRegions[{p1,p2,..pk},{x1,x2,...xn}, a] detects regions in the \
asymptotic expansion of the integral \
\!\(\*FormBox[RowBox[{RowBox[{RowBox[{RowBox[{RowBox[{RowBox[{UnderscriptBox[\
StyleBox[\"\[Integral]\", \"TI\"], \
SubsuperscriptBox[\"\[DoubleStruckCapitalR]\", \"+\", \"n\"]], \
StyleBox[SubscriptBox[\"dx\", \"1\"], \"TI\"]}], StyleBox[\"...\", \"TI\"]}], \
SubscriptBox[StyleBox[\"dx\", \"TI\"], \"n\"], SubsuperscriptBox[StyleBox[\"p\
\", \"TI\"], \"1\", SubscriptBox[\"\[Alpha]\", \"1\"]]}], \"...\"}], \
SubsuperscriptBox[StyleBox[\"p\", \"TI\"], \"k\", SubscriptBox[\"\[Alpha]\", \
\"k\"]], \" \", \"in\", \" \", \"the\", \" \", \"limit\", \" \", StyleBox[\"a\
\", \"TI\"]}], \"\[Rule]\", \"0\"}],TraditionalForm]\). \nBy default, the \
result is the scaling powers of polynomials and variables for each \
region.\>\""}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"GetRegions", "::", "poly"}], 
   "=", "\"\<GetRegions works only for polynomial functions of both variables \
and small parameters.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"GetRegions", "::", "zero"}], 
   "=", "\"\<The integral is scaleless! Returning scaling\>\""}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Options", "[", "GetRegions", "]"}], "=", 
   RowBox[{"{", 
    RowBox[{"OutputForm", "->", "Exponent"}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"GetRegions", "[", 
   RowBox[{"polys_List", ",", "vars_List", ",", "p_Symbol", ",", 
    RowBox[{"OptionsPattern", "[", "]"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"normals", ",", 
      RowBox[{"pvs", "=", 
       RowBox[{"Prepend", "[", 
        RowBox[{"vars", ",", "p"}], "]"}]}], ",", 
      RowBox[{"nv", "=", 
       RowBox[{"Length", "@", "vars"}]}], ",", "ufold", ",", "scalings"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"!", 
        RowBox[{"AllTrue", "[", 
         RowBox[{"polys", ",", 
          RowBox[{
           RowBox[{"PolynomialQ", "[", 
            RowBox[{"#", ",", "pvs"}], "]"}], "&"}]}], "]"}]}], ",", 
       RowBox[{
        RowBox[{"Message", "[", 
         RowBox[{"GetRegions", "::", "poly"}], "]"}], ";", 
        RowBox[{"Return", "[", "$Failed", "]"}]}]}], "]"}], ";", "\n", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "If", " ", "the", " ", "integral", " ", "is", " ", "just", " ", 
        "proportional", " ", "to", " ", "a", " ", "power", " ", "of", " ", 
        "small", " ", "parameter"}], ",", " ", 
       RowBox[{
       "the", " ", "polynomial", " ", "should", " ", "be", " ", "homogeneous",
         " ", "wrt", " ", "some", " ", "scaling"}]}], "*)"}], "\n", 
     RowBox[{"normals", "=", 
      RowBox[{"NullSpace", "[", 
       RowBox[{"Differences", "[", 
        RowBox[{"First", "/@", 
         RowBox[{"CoefficientRules", "[", 
          RowBox[{
           RowBox[{"Times", "@@", "polys"}], ",", "pvs"}], "]"}]}], "]"}], 
       "]"}]}], ";", "\n", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"normals", "=!=", 
        RowBox[{"{", "}"}]}], ",", "\n", 
       RowBox[{"normals", "=", 
        RowBox[{"DeleteCases", "[", 
         RowBox[{
          RowBox[{"RowReduce", "[", "normals", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", "___"}], "}"}]}], "]"}]}], ",", "\n", 
       RowBox[{"normals", "=", 
        RowBox[{"Cases", "[", 
         RowBox[{
          RowBox[{"InnerNormals", "[", 
           RowBox[{"First", "/@", 
            RowBox[{"CoefficientRules", "[", 
             RowBox[{
              RowBox[{"Times", "@@", "polys"}], ",", "pvs"}], "]"}]}], "]"}], 
          ",", 
          RowBox[{
           RowBox[{"s", ":", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"_", "?", "Positive"}], ",", "__"}], "}"}]}], ":>", 
           FractionBox["s", 
            RowBox[{"First", "[", "s", "]"}]]}]}], "]"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"scalings", "=", 
      RowBox[{"MapThread", "[", 
       RowBox[{"List", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Transpose", "@", 
           RowBox[{"Outer", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Min", "[", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"First", "/@", 
                  RowBox[{"CoefficientRules", "[", 
                   RowBox[{"#", ",", "pvs"}], "]"}]}], ")"}], ".", "#2"}], 
               "]"}], "&"}], ",", "polys", ",", "normals", ",", "1"}], 
            "]"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{"Rest", "/@", "normals"}]}], "}"}]}], "]"}]}], ";", "\n", 
     RowBox[{"Replace", "[", 
      RowBox[{
       RowBox[{"OptionValue", "[", "OutputForm", "]"}], ",", 
       RowBox[{"{", "\n", 
        RowBox[{
         RowBox[{"Rule", "->", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"Thread", "[", 
                RowBox[{"polys", "->", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{"p", "^", 
                    RowBox[{"(", "#1", ")"}]}], ")"}], 
                  RowBox[{"Factor", "[", 
                   RowBox[{
                    FractionBox["polys", 
                    RowBox[{"(", 
                    RowBox[{"p", "^", "#1"}], ")"}]], "/.", 
                    RowBox[{"Thread", "[", 
                    RowBox[{"vars", "->", 
                    RowBox[{
                    RowBox[{"p", "^", "#2"}], " ", "vars"}]}], "]"}]}], 
                   "]"}]}]}], "]"}], ",", 
               RowBox[{"Thread", "[", 
                RowBox[{"vars", "->", 
                 RowBox[{
                  RowBox[{"p", "^", "#2"}], " ", "vars"}]}], "]"}]}], "}"}], 
             "&"}], "@@@", "scalings"}], ")"}]}], ",", "\n", 
         RowBox[{"RuleDelayed", ":>", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"Function", "[", 
                RowBox[{
                 RowBox[{"{", 
                  RowBox[{"pp", ",", "vp"}], "}"}], ",", 
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{"Thread", "[", 
                    RowBox[{
                    SuperscriptBox["polys", "C_"], ":>", 
                    RowBox[{"Evaluate", "[", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"p", "^", 
                    RowBox[{"(", 
                    RowBox[{"pp", "*", "C"}], ")"}]}], ")"}], 
                    SuperscriptBox[
                    RowBox[{"Factor", "[", 
                    RowBox[{
                    FractionBox["polys", 
                    RowBox[{"(", 
                    RowBox[{"p", "^", "pp"}], ")"}]], "/.", 
                    RowBox[{"Thread", "[", 
                    RowBox[{"vars", "->", 
                    RowBox[{
                    RowBox[{"p", "^", "vp"}], " ", "vars"}]}], "]"}]}], "]"}],
                     "C"]}], "]"}]}], "]"}], ",", 
                   RowBox[{"Thread", "[", 
                    RowBox[{"vars", "->", 
                    RowBox[{
                    RowBox[{"p", "^", "vp"}], " ", "vars"}]}], "]"}]}], 
                  "}"}]}], "]"}], "@@@", "scalings"}], ")"}], "&"}], "[", 
            RowBox[{"Unique", "[", "\"\<n\>\"", "]"}], "]"}], ")"}]}], ",", 
         "\n", 
         RowBox[{"_", "->", "scalings"}]}], "}"}]}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}], "\n", 
 RowBox[{
  RowBox[{"GetRegions", "[", 
   RowBox[{"poly_", ",", "vars_List", ",", "p_Symbol", ",", 
    RowBox[{"opts", ":", 
     RowBox[{"OptionsPattern", "[", "]"}]}]}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"First", "[", "#1", "]"}], ",", "#2"}], "}"}], "&"}], "@@@", 
   RowBox[{"GetRegions", "[", 
    RowBox[{
     RowBox[{"{", "poly", "}"}], ",", "vars", ",", "p", ",", "opts"}], 
    "]"}]}]}]}], "Code",
